function action(a, b,) {
    let max_delimeter=-9999999
    function check_small_number(a, b) {
        if (a <= b) {
            return a
        } else {
            return b
        }
    }
    let small_number=check_small_number(a,b)
    function isInt(n) {
        return n%1===0
    }

    for (let index = 1; index <= small_number; index++) {
        if (isInt(a/index) && isInt(b/index) && index > max_delimeter) {
            max_delimeter=index
        }        
    }
    console.log(max_delimeter)
}
