function solve(num) {
    let number = num.toString().split('')
    let firstNumber = number[0]
    let sum = 0
    let same = true
    number.forEach(element => {
        if (firstNumber === element) {
            sum += Number(element)
        } else {
            same = false
            sum += Number(element)
        }
    });
    if (same) { console.log(same) }
    else { console.log(same) }
    console.log(sum)
}
