function action(...elements) {
    let number = parseInt(elements[0])
    function cooking(command) {
        if (command === 'chop') { number /= 2; return number }
        else if (command === 'dice') { number = Math.sqrt(number); return number }
        else if (command === 'spice') { number += 1; return number }
        else if (command === 'bake') { number *= 3; return number }
        else if (command === 'fillet') { number -= number * 0.2; return number }
    }
    console.log(cooking(elements[1]))
    console.log(cooking(elements[2]))
    console.log(cooking(elements[3]))
    console.log(cooking(elements[4]))
    console.log(cooking(elements[5]))
}
